<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="ddocdoc.mapper.PresMapper">
	<cache />
	
	<!-- 처방전 등록 -->
	<insert id="hospitalInsert" parameterType="HospitalVO">
		insert into dd_hospital values('H00' || hospital_seq.nextval, #{hos_name}, #{hos_tel},
			#{hos_addr}, #{hos_time}, #{hos_info}, 0, #{hos_type})
	</insert>
</mapper>