<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">



<mapper namespace="ddocdoc.mapper.ChildHeightMapper">
	<cache />
	
	<!-- 아이번호 가져오기 -->
	<select id="selectChildNum" parameterType="String" resultType="String">
	select ch_num
	from dd_child
	where cus_num = #{cus_num}
	</select>
	
</mapper>















